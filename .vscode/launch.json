{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "console": "internalConsole",
      "program": "${workspaceFolder}/main.go",
    },
    {
      "name": "Wails: Debug mousk",
      "type": "go",
      "request": "launch",
      "console": "internalConsole",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/mousk.exe",
      "preLaunchTask": "build debug",
      // "preLaunchTask": "wails dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails3 Dev",
      "type": "go",
      "request": "launch",
      "console": "internalConsole",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/mousk.exe",
      // "preLaunchTask": "build debug",
      "preLaunchTask": "wails dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Attach to running Go process",
      "type": "go",
      "debugAdapter": "dlv-dap",
      "request": "attach",
      "processId": "mousk.exe",
      // "preLaunchTask": "wails dev"
    },
    // {
    //   "name": "Wails: Dev",
    //   "request": "launch",
    //   "console": "internalConsole",
    //   "mode": "exec",
    //   "program": "${workspaceFolder}/build/bin/mousk.exe",
    //   "cwd": "${workspaceFolder}"
    // },
    {
      "name": "Wails: Production mousk",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/mousk.exe",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Dev mousk",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/mousk.exe",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}